{
  "name": "@dreesq/medusa",
  "version": "1.0.0",
  "description": "React application that uses serpent, providing perfect start application along with admin panel.",
  "main": "index.js",
  "scripts": {
    "server:start": "node index.js",
    "server:dev": "nodemon index.js",
    "client:dev": "cd ./client && react-scripts start",
    "client:prod": "cd ./client && react-scripts build",
    "dev": "concurrently --kill-others \"npm run server:dev\" \"npm run client:dev\"",
    "prod": "npm run client:prod && cross-env NODE_ENV=production npm run server:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreesq/medusa.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dreesq/medusa/issues"
  },
  "homepage": "https://github.com/dreesq/medusa#readme",
  "dependencies": {
    "@dreesq/serpent": "^1.2.3",
    "@dreesq/serpent-client": "^1.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
